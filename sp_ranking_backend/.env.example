# Backend environment configuration

# Finnhub API
FINNHUB_API_KEY=

# Database
DB_URL=sqlite:///./data/app.db

# Server
BACKEND_PORT=8000

# Processing and caching
BATCH_SIZE=25
MAX_CONCURRENCY=5
CACHE_TTL_MINUTES=1440

# Export temp directory
EXPORT_TMP_DIR=./exports

# CORS
# Comma-separated list, or * for all
ALLOWED_ORIGINS=*
