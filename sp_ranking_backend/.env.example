# Finnhub API key for fetching financial/market data
FINNHUB_API_KEY=your_finnhub_api_key_here

# Database URL. For SQLite use sqlite:///./data/app.db
DB_URL=sqlite:///./data/app.db

# Backend server port
BACKEND_PORT=8000

# Processing batch parameters (tune later during implementation)
BATCH_SIZE=25
MAX_CONCURRENCY=5

# Cache TTL for cached API data (minutes)
CACHE_TTL_MINUTES=1440

# Directory for temporary export files
EXPORT_TMP_DIR=./exports

# CORS allowed origins (comma-separated). Example: http://localhost:3000,https://yourdomain.com
ALLOWED_ORIGINS=*
